def print_diagonals(matrix):
    n = len(matrix)
    main_diagonal_sum = 0
    print("Main Diagonal:")
    for i in range(n):
        print(matrix[i][i], end=" ")
        main_diagonal_sum += matrix[i][i]
    print()
    print("Sum of main diagonal:", main_diagonal_sum)
matrix = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]
print_diagonals(matrix)

output:
Main Diagonal:
1 5 9 
Sum of main diagonal: 15
